<%if @current_user%>
	<%unless shared %>
		<li class='favoriteThis'>	
			<%=link_to image_tag("/images/facebook_this.png", :alt=>t("common.facebook.label")), url, :rel => 'nofollow', :title=>t("common.facebook.label") %>
		</li>
	<%else%>
		<li class='favoritedThis'>
			<%=image_tag("/images/facebooked_this.png", :alt=>t("common.facebooked"))%>
		</li>
	<%end%>
<%end%>